    "@zeit/next-sass": "^1.0.1",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "clsx": "^1.0.4",
    "express": "^4.17.1",
    "hoist-non-react-statics": "3.3.0",
    "i18next": "^17.0.18",
    "invariant": "2.2.4",
    "lodash": "^4.17.21",
    "next": "^9.0.7",
    "next-i18next": "^1.2.1",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "7.0.2",
    "redux": "4.0.1",
    "redux-form": "^8.2.4",
    "redux-saga": "1.0.2",
    "redux-saga-routines": "^3.1.3",
    "reselect": "4.0.0",
    "signale": "^1.4.0",
    "typeface-metropolis": "^0.0.74"
  },
  "devDependencies": {
    "@babel/cli": "7.4.3",
    "@babel/core": "7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
