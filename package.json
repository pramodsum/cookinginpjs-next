{
  "name": "react-next-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Jorge Luis Calleja <jorgelca1205@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "node ./server/app.js app",
    "build": "next build app",
    "start": "NODE_ENV=production node ./server/app.js",
    "test:unit": "NODE_ENV=test jest --coverage",
    "test": "npm run test:unit",
    "test:watch": "NODE_ENV=test jest --watchAll",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "prettier": "prettier --ignore-path \"./.prettierignore\" --config .prettierrc --write \"./app/**/*.js\" \"./app/**/*.jsx\"",
    "lint": "npm run lint:eslint",
    "lint:eslint": "eslint --fix .",
    "lint:husky": "npm run prettier && npm run lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:husky"
    }
  },
  "dependencies": {
    "@emotion/core": "^10.0.21",
    "@emotion/styled": "^10.0.17",
    "@typefaces-pack/typeface-inter": "^1.0.3",
